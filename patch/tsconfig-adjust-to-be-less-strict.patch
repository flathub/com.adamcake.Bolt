From 902abba79fe204196775752ab60b26961c869dda Mon Sep 17 00:00:00 2001
From: nmlynch94 <nmlynch94@gmail.com>
Date: Sun, 26 Oct 2025 12:33:34 -0400
Subject: [PATCH] tsconfig adjust to be less strict

---
 tools/typescript/tsconfig_base.json | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/tools/typescript/tsconfig_base.json b/tools/typescript/tsconfig_base.json
index e67b7e261f..251728437f 100644
--- a/tools/typescript/tsconfig_base.json
+++ b/tools/typescript/tsconfig_base.json
@@ -3,7 +3,7 @@
     "module": "ESNext",
     "target": "ESNext",
 
-    "noEmitOnError": true,
+    "noEmitOnError": false,
     "pretty": true,
 
     "allowUnreachableCode": false,
@@ -15,7 +15,7 @@
     "noUncheckedIndexedAccess": true,
     "noUncheckedSideEffectImports": true,
     "noUnusedLocals": true,
-    "noUnusedParameters": true,
+    "noUnusedParameters": false,
     "preserveSymlinks": true,
     "strict": true,
 
-- 
2.51.1.dirty

