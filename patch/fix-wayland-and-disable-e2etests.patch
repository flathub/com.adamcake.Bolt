From 703aabf20ebd3df238b60769d76e8bc687e33742 Mon Sep 17 00:00:00 2001
From: nmlynch94 <nmlynch94@gmail.com>
Date: Thu, 9 Oct 2025 13:21:39 -0400
Subject: [PATCH] fix wayland and disable e2etests

---
 chrome/browser/glic/e2e_test/BUILD.gn | 5 ++---
 ui/ozone/platform/wayland/wayland.gni | 2 +-
 2 files changed, 3 insertions(+), 4 deletions(-)

diff --git a/chrome/browser/glic/e2e_test/BUILD.gn b/chrome/browser/glic/e2e_test/BUILD.gn
index 0ba0b3fa62..54575bf93c 100644
--- a/chrome/browser/glic/e2e_test/BUILD.gn
+++ b/chrome/browser/glic/e2e_test/BUILD.gn
@@ -6,6 +6,8 @@ import("//build/config/ozone.gni")
 import("//build/config/ui.gni")
 import("//testing/test.gni")
 
+checkout_glic_e2e_tests = false
+
 source_set("internal_test_placeholder_constants") {
   testonly = true
   sources = [ "internal_test_placeholder_constants.h" ]
@@ -18,9 +20,6 @@ source_set("e2e_test_support") {
     "glic_e2e_test.cc",
     "glic_e2e_test.h",
   ]
-  if (checkout_glic_e2e_tests) {
-    defines += [ "ENABLE_GLIC_INTERNAL_TESTS=1" ]
-  }
 
   # Internal targets also use the dependencies by depending on this target.
   public_deps = [
diff --git a/ui/ozone/platform/wayland/wayland.gni b/ui/ozone/platform/wayland/wayland.gni
index 01e5517f73..7144a841ca 100644
--- a/ui/ozone/platform/wayland/wayland.gni
+++ b/ui/ozone/platform/wayland/wayland.gni
@@ -18,7 +18,7 @@ declare_args() {
   use_bundled_weston = false
 
   # Checks if mutter must be compiled
-  use_bundled_mutter = checkout_mutter
+  use_bundled_mutter = false
 }
 
 assert(
